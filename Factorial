import java.util.*;
class factotial {
    public static void main(String args[])
    {
        Scanner sc = new Scanner (System.in);
        System.out.println("Enter a number");
        
        int n , fact = 1;
        n = sc.nextInt();
        
        for (int i = 1; i <= n ; i ++)
        {
            fact = fact * i;
        }
        System.out.println("Fcatorial of " +n + " is = " +fact);
    }
}
