class product 
{
    private String product_id;
    private String name;
    private int price;
    
    product (String product_id, String name, int price)
    {
        this.product_id = product_id;
        this.name = name;
        this.price = price;
    }
    
    void display ()
    {
        System.out.println("Product Id : " + product_id);
        System.out.println("Nmae : " + name);
        System.out.println("Price : " + price);
    }
    
    public static void main(String args[])
    {
        product p1 = new product ("1001","Product - 1",2344);
        product p2 = new product ("1002","Product - 2",5788);
        p1.display();
        p2.display();
    }
}
